{
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "utils/*": ["src/utils/*"],
      "styles/*": ["src/styles/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"],
      "ui/*": ["src/ui/*"],
      "assets/*": ["src/assets/*"],
      "models/*": ["src/models/*"]
    }
  }
}
